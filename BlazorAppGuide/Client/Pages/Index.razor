@page "/"
@inject IMovieApi movieApi          

@using Model.Entitties

<div>
    <h3>Movies</h3>
    <MoviesListComponent Movies="Movies" />   
</div>

@code {
    private List<Movie> Movies { get; set; }

    protected override async Task OnInitializedAsync()
    {
        Movies = movieApi.GetMovies();
    }
}
